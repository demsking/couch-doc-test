msgstr ""

# 38348f220de64cf498f5f0e50af5f791
#: ../../../src/whatsnew/0.8.rst:91
msgid "The `couchdb` and `couchjs` scripts have been improved for portability."
msgstr ""

# d875135e99ca4c2b920bba69e87b3bd9
#: ../../../src/whatsnew/0.8.rst:92
msgid "The build and system integration have been improved for portability."
msgstr ""

# 405ea9aaeecf4a779dbcd90e93884ee6
#: ../../../src/whatsnew/0.8.rst:97
msgid "The view engine has been completely decoupled from the storage engine. Index data is now stored in separate files, and the format of the main database file has changed."
msgstr ""

# 0c43a7f326454a2eba91e66cb49f24c8
#: ../../../src/whatsnew/0.8.rst:100
msgid "Databases can now be compacted to reclaim space used for deleted documents and old document revisions."
msgstr ""

# 0e35a84468004d4e81a672d973623ee6
#: ../../../src/whatsnew/0.8.rst:102
msgid "Support for incremental map/reduce views has been added."
msgstr ""

# 43c100860d9a46daa5c0bb5327c11c03
#: ../../../src/whatsnew/0.8.rst:103
msgid "To support map/reduce, the structure of design documents has changed. View values are now JSON objects containing at least a `map` member, and optionally a `reduce` member."
msgstr ""

# 6002001c889d4b00bcd13cd4a9039d67
#: ../../../src/whatsnew/0.8.rst:106
msgid "View servers are now identified by name (for example `javascript`) instead of by media type."
msgstr ""

# 9e7d224dd85343acbb8ad8bf8d6c3290
#: ../../../src/whatsnew/0.8.rst:108
msgid "Automatically generated document IDs are now based on proper UUID generation using the crypto module."
msgstr ""

# d6dcbf6e12c24ec49ea1031b2074f664
#: ../../../src/whatsnew/0.8.rst:110
msgid "The field `content-type` in the JSON representation of attachments has been renamed to `content_type` (underscore)."
msgstr ""

# 5c5d66fbf9414f2f84c7573c9bfb5ced
#: ../../../src/whatsnew/0.8.rst:116
msgid "When adding a field to a document, Futon now just adds a field with an autogenerated name instead of prompting for the name with a dialog. The name is automatically put into edit mode so that it can be changed immediately."
msgstr ""

# 58b8527141b0404992d810a59e474ca2
#: ../../../src/whatsnew/0.8.rst:119
msgid "Fields are now sorted alphabetically by name when a document is displayed."
msgstr ""

# 6581b376f793433980347d8ac498bdb4
#: ../../../src/whatsnew/0.8.rst:120
msgid "Futon can be used to create and update permanent views."
msgstr ""

# 43067ae6d2b046069eec302cb3c96ad0
#: ../../../src/whatsnew/0.8.rst:121
msgid "The maximum number of rows to display per page on the database page can now be adjusted."
msgstr ""

# 0e2f253b706e4de8adfb931deadf031f
#: ../../../src/whatsnew/0.8.rst:123
msgid "Futon now uses the XMLHTTPRequest API asynchronously to communicate with the CouchDB HTTP server, so that most operations no longer block the browser."
msgstr ""

# 4ec3932449f9409ea3a54209c2b2d07b
#: ../../../src/whatsnew/0.8.rst:125
msgid "View results sorting can now be switched between ascending and descending by clicking on the `Key` column header."
msgstr ""

# 632e9164227e4d1898753b47a18fa621
#: ../../../src/whatsnew/0.8.rst:127
msgid "Fixed a bug where documents that contained a `@` character could not be viewed. (:issue:`12`)"
msgstr ""

# 1747b2efd10948b2bcd5e88b41116838
#: ../../../src/whatsnew/0.8.rst:129
msgid "The database page now provides a `Compact` button to trigger database compaction. (:issue:`38`)"
msgstr ""

# f2d0ed142e8344439968489834eed88f
#: ../../../src/whatsnew/0.8.rst:131
msgid "Fixed portential double encoding of document IDs and other URI segments in many instances. (:issue:`39`)"
msgstr ""

# e25c6ff4211647c7a09777da70001f5f
#: ../../../src/whatsnew/0.8.rst:133
msgid "Improved display of attachments."
msgstr ""

# e1911259ad034cae97bbe774fcc9566c
#: ../../../src/whatsnew/0.8.rst:134
msgid "The JavaScript Shell has been removed due to unresolved licensing issues."
msgstr ""

# d648320bceff457e84e7c54570614110
#: ../../../src/whatsnew/0.8.rst:139
msgid "SpiderMonkey is no longer included with CouchDB, but rather treated as a normal external dependency. A simple C program (`_couchjs`) is provided that links against an existing SpiderMonkey installation and uses the interpreter embedding API."
msgstr ""

# d2b6860f56354257ab6de57723f794be
#: ../../../src/whatsnew/0.8.rst:143
msgid "View functions using the default JavaScript view server can now do logging using the global `log(message)` function. Log messages are directed into the CouchDB log at `INFO` level. (:issue:`59`)"
msgstr ""

# 54d63d16985340f8893db7d55b9551d9
#: ../../../src/whatsnew/0.8.rst:146
msgid "The global `map(key, value)` function made available to view code has been renamed to `emit(key, value)`."
msgstr ""

# 6372a9e14299443dbab8f1e4dd37c341
#: ../../../src/whatsnew/0.8.rst:148
msgid "Fixed handling of exceptions raised by view functions."
msgstr ""

# bdc1a248e14943bc9d19b0e5bc81a4a5
#: ../../../src/whatsnew/0.8.rst:153
msgid "CouchDB now uses MochiWeb instead of inets for the HTTP server implementation. Among other things, this means that the extra configuration files needed for inets (such as `couch_httpd.conf`) are no longer used."
msgstr ""

# fec7a3bc52824a9eaf4156a78026db7b
#: ../../../src/whatsnew/0.8.rst:156
msgid "The HTTP interface now completely supports the `HEAD` method. (:issue:`3`)"
msgstr ""

# 8f41437fb07944629338b7de8148490a
#: ../../../src/whatsnew/0.8.rst:157
msgid "Improved compliance of `Etag` handling with the HTTP specification. (:issue:`13`)"
msgstr ""

# 3ecdbc5b78334248b88289aae20b6d8b
#: ../../../src/whatsnew/0.8.rst:159
msgid "Etags are no longer included in responses to document `GET` requests that include query string parameters causing the JSON response to change without the revision or the URI having changed."
msgstr ""

# beef0e24fc28410ca2a605f0baf75c74
#: ../../../src/whatsnew/0.8.rst:162
msgid "The bulk document update API has changed slightly on both the request and the response side. In addition, bulk updates are now atomic."
msgstr ""

# 77c8aed7fb104fc98f0804c31ac8e123
#: ../../../src/whatsnew/0.8.rst:164
msgid "CouchDB now uses `TCP_NODELAY` to fix performance problems with persistent connections on some platforms due to nagling."
msgstr ""

# ae41dd2809e849dca22105c2c707eda8
#: ../../../src/whatsnew/0.8.rst:166
msgid "Including a `?descending=false` query string parameter in requests to views no longer raises an error."
msgstr ""

# f5dba5ee0bbf40e1a6ca436cf57ec619
#: ../../../src/whatsnew/0.8.rst:168
msgid "Requests to unknown top-level reserved URLs (anything with a leading underscore) now return a `unknown_private_path` error instead of the confusing `illegal_database_name`."
msgstr ""

# 8934294e09fc40aab2c78c87cd58a6d8
#: ../../../src/whatsnew/0.8.rst:171
msgid "The Temporary view handling now expects a JSON request body, where the JSON is an object with at least a `map` member, and optional `reduce` and `language` members."
msgstr ""

# 3125d0c8f10449769d124eb61a642595
#: ../../../src/whatsnew/0.8.rst:174
msgid "Temporary views no longer determine the view server based on the Content-Type header of the `POST` request, but rather by looking for a `language` member in the JSON body of the request."
msgstr ""

# 27850cd01c454707a13bb96f3219ed8b
#: ../../../src/whatsnew/0.8.rst:177
msgid "The status code of responses to `DELETE` requests is now 200 to reflect that that the deletion is performed synchronously."
msgstr ""
