#: ../../../src/api/document/common.rst:380
msgid "Attachments"
msgstr ""

# d20bdc545ef940ce88960ee64258d1e4
#: ../../../src/api/document/common.rst:382
msgid "If the document includes attachments, then the returned structure will contain a summary of the attachments associated with the document, but not the attachment data itself."
msgstr ""

# c628cfe4b0fb408bbb59c85df8616cab
#: ../../../src/api/document/common.rst:386
msgid "The JSON for the returned document will include the ``_attachments`` field, with one or more attachment definitions."
msgstr ""

# bdf2d2cad5db41649600a1880059f4dd
#: ../../../src/api/document/common.rst:389
msgid "The ``_attachments`` object keys are attachments names while values are information objects with next structure:"
msgstr ""

# eab2567e88a6413fb666765cbf226966
#: ../../../src/api/document/common.rst:392
msgid "**content_type** (*string*): Attachment MIME type"
msgstr ""

# 5c4108a5c69146c097c10c16402314b5
#: ../../../src/api/document/common.rst:393
msgid "**data** (*string*): Base64-encoded content. Available if attachment content requested by using ``attachments=true`` or ``atts_since`` query parameters"
msgstr ""

# 5ba29fa7234949fb997fb6749b5e9c82
#: ../../../src/api/document/common.rst:395
msgid "**digest** (*string*): Content hash digest. It starts with prefix which announce hash type (``md5-``) and continues with Base64-encoded hash digest"
msgstr ""

# 13c4ca67e01f424bac5625061739c236
#: ../../../src/api/document/common.rst:398
msgid "**encoded_length** (*number*): Compressed attachment size in bytes Available when query parameter ``att_encoding_info=true`` was specified and ``content_type`` is in :config:option:`list of compressiable types <attachments/compressible_types>`"
msgstr ""

# baf60d1525d34078b3b28d0822f6a71a
#: ../../../src/api/document/common.rst:402
msgid "**encoding** (*string*): Compression codec. Available when query parameter ``att_encoding_info=true`` was specified"
msgstr ""

# 4d5f71466d26410b84cd5b7316e308df
#: ../../../src/api/document/common.rst:404
msgid "**length** (*number*): Real attachment size in bytes. Not available if attachment content requested"
msgstr ""

# 71c0a30a612d49c488ddddf8dd4174d7
#: ../../../src/api/document/common.rst:406
msgid "**revpos** (*number*): Revision *number* when attachment was added"
msgstr ""

# ea0a96fdeab140899250b838fcc6f1fe
#: ../../../src/api/document/common.rst:407
msgid "**stub** (*boolean*): Has ``true`` value if object contains stub info and no content. Otherwise omitted in response"
msgstr ""

